[OCCA]
backend = SERIAL
deviceNumber = LOCAL-RANK

[GENERAL] 
polynomialOrder = 7
numSteps = 20000 
dt = 1e-04
timeStepper = tombo2
writeInterval = 100

[PROBLEMTYPE]
variableProperties = yes

[PRESSURE]
residualTol = 1e-05

[VELOCITY]
boundaryTypeMap = v, O, W, symx, symy
residualTol = 1e-06

[TEMPERATURE]
boundaryTypeMap = t, I, I, I, I
residualTol = 1e-04

[SCALAR01]
solver = none
boundaryTypeMap = none
writeToFile = true
